{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-snake-case"
    ],
    "prefer-const": false,
    "no-console": false,
    "arrow-parens": false,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-trailing-whitespace": [true, "ignore-blank-lines"],
    "interface-name": [true, "never-prefix"],
    "ordered-imports": false
  },
  "rulesDirectory": []
}
