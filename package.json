{
  "name": "indexer",
  "version": "0.4.1",
  "scripts": {
    "clean:dist": "rimraf dist/*",
    "build": "npm run clean:dist && tsc && cp package.json dist",
    "prod": "node dist/main.js",
    "start": "npm run build && concurrently \"tsc -w\" \"npm run watch\"",
    "watch": "sleep 3 && nodemon --watch dist dist/main.js",
    "test:unit": "alsatian src/**/*.spec.ts",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.ts'",
    "test": "npm run lint && npm run test:unit",
    "check-coverage": "nyc npm run test"
  },
  "dependencies": {
    "@sentry/node": "5.10.2",
    "axios": "^0.18.1",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "inversify": "^5.0.1",
    "inversify-express-utils": "^6.3.2",
    "mongodb": "^3.3.2",
    "pg": "^7.7.1",
    "puppeteer": "^1.11.0",
    "reflect-metadata": "^0.1.12",
    "saslprep": "^1.0.3",
    "typescript": "^3.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/cheerio": "^0.22.16",
    "@types/express": "^4.17.2",
    "@types/mongodb": "^3.3.6",
    "@types/node": "^10.12.12",
    "@types/pg": "^7.4.11",
    "@types/puppeteer": "^1.11.0",
    "alsatian": "^3.0.1",
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1"
  },
  "resolutions": {
    "minimist": "^0.2.1",
    "kind-of": "^6.0.3"
  }
}
